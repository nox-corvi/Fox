{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "XLog": {
    "Target": "Echo",
    "Echo": {
      "Enabled": "true",
      "Colorized": "true"
    },
    "Level": "Trace",
    "AllowedHosts": "*"
  }
}
